.row
  .col-md-12.p-t-15
    %section.panel
      %header.panel-heading
        Edit Project
      .panel-body
        = form_for @project, url: admin_user_project_path(current_user, @project), html: {class: 'create_project'}, method: :put do |form|
          = render 'form', project: @project, form: form
